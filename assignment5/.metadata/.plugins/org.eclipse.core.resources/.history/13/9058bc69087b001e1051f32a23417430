package que2;

public class Queue {
	private final int size;
	private int front;
	private int rear;
	private int arr[];

	public Queue(int size) {

		this.front = -1;
		this.rear = -1;
		this.size=size;
		arr = new int[size];
	}
	public boolean isEmpty() 
	{
	  return rear==front;	
	}
   public boolean isFull() 
   {
	return rear==size-1;   
   }
   public void push(int value) 
   {
	if(isFull())
		System.out.println("Queue is empty");
	else {
		rear++;
		arr[rear]=value;}
	
   }
   public void pop() 
   {
	if(isEmpty())
	System.out.println("Queue is empty");
	else 
	{
	 front++;	
	}
   }
   public int 
}
