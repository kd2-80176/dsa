package que2;

public class Stack {
	private int size;
	private int top;
	private int arr[];
	
	LinkedList list=new LinkedList();

	public Stack(int size) {
		this.size = size;
		top=-1;
		int arr[]=new int[size];
		
	}
	public boolean isEmpty() {
		return top==-1;
		
	}
	public boolean isFull()
	{
	   return top==size-1;	
	}
	public void push(int value) 
	{
	  list.addLast(value);
	}
	public void pop() 
	{
	  list.delLast();	
	}
	public void peek() 
	{
	
	}
	

}
